import "../stdlib/journal_tag.ğŸ†"
import "../stdlib/iostream.ğŸ†"
import "../stdlib/list.ğŸ†"
import "../stdlib/integer.ğŸ†"

struct Main {
	strong OutputStream out;
	constructor(){
		out = new StdOut();

		var int i = 4;
		auto t = new JournalTag<int>(i);
		t.bring_back();
	};
	fun void main(){
		var List<Integer> list = new List<Integer>();
		list.push_back(new Integer(1));
		list.print(out);
		out.print_newline();
		var JournalTag<List<Integer>> s = new JournalTag<List<Integer>>(list);
		list.push_back(new Integer(2));
		list.print(out);
		out.print_newline();
		var JournalTag<List<Integer>> s2 = new JournalTag<List<Integer>>(list);
		s.bring_back();
		list.print(out);
		out.print_newline();
		list.push_back(new Integer(3));
		list.print(out);
		out.print_newline();
		s2.bring_back();
		list.print(out);
		out.print_newline();
	};
}
